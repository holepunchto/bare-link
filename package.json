{
  "name": "bare-link",
  "version": "2.0.2",
  "description": "Native addon linker for Bare",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js",
    "./preset": "./lib/preset.js",
    "./preset/*": "./lib/preset/*.js"
  },
  "bin": {
    "bare-link": "bin.js"
  },
  "files": [
    "index.js",
    "bin.js",
    "CMakeLists.txt",
    "cmake",
    "lib",
    "prebuilds"
  ],
  "imports": {
    "#patchelf": {
      "darwin": {
        "arm64": "./prebuilds/darwin-arm64/patchelf",
        "x64": "./prebuilds/darwin-x64/patchelf"
      },
      "linux": {
        "arm64": "./prebuilds/linux-arm64/patchelf",
        "x64": "./prebuilds/linux-x64/patchelf"
      },
      "win32": "./prebuilds/win32-x64/patchelf.exe"
    },
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "url": {
      "bare": "bare-url",
      "default": "url"
    }
  },
  "scripts": {
    "test": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-link.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-link/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-link#readme",
  "dependencies": {
    "bare-fs": "^4.0.0",
    "bare-module-resolve": "^1.12.0",
    "bare-os": "^3.2.0",
    "bare-path": "^3.0.0",
    "bare-subprocess": "^5.0.2",
    "bare-url": "^2.0.9",
    "paparam": "^1.5.0",
    "require-asset": "^1.1.0"
  },
  "devDependencies": {
    "brittle": "^3.2.1",
    "cmake-bare": "^1.7.0",
    "cmake-ports": "^1.2.3",
    "prettier": "^3.3.3",
    "prettier-config-holepunch": "^2.0.0"
  }
}
